<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>rss news</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700" rel="stylesheet" />
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body>
    <header>
      <h1>rss news</h1>
    </header>
    <main>
      <div class="cards">


        <% for (let i = 0; i < news.length; i++) { %>


        <div class="card">
          <div class="title">
            <h2><%= news[i].title %></h2>
          </div>
          <ul>
            <% news[i].items.forEach((item) => { %>
            <li><a href="<%= item.link %>"><%= item.title %></a></li>
            <% }); %>
          </ul>
        </div>

        <% } %>

    </div>
    </main>
    <footer>
      <p>from <a href="https://github.com/semiformal-net/rssboxes">semiformal</a></p>
    </footer>
  </body>
</html>
